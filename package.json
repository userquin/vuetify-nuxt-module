{
  "name": "vuetify-nuxt-module",
  "type": "module",
  "version": "0.16.1",
  "packageManager": "pnpm@9.5.0",
  "description": "Zero-Config Nuxt Module for Vuetify",
  "author": "userquin <userquin@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/userquin/vuetify-nuxt-module#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/userquin/vuetify-nuxt-module.git"
  },
  "bugs": "https://github.com/userquin/vuetify-nuxt-module/issues",
  "keywords": [
    "nuxt",
    "nuxt 3",
    "vuetify",
    "vuetify 3",
    "nuxt-module"
  ],
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "default": "./dist/module.mjs"
    },
    "./custom-configuration": {
      "types": "./custom-configuration.d.ts",
      "default": "./custom-configuration.mjs"
    },
    "./configuration": {
      "types": "./configuration.d.ts"
    },
    "./*": "./*"
  },
  "main": "./dist/module.mjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist",
    "*.cjs",
    "*.d.ts",
    "*.mjs"
  ],
  "scripts": {
    "prepack": "nuxt-module-build prepare && nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:multiple-json": "MULTIPLE_LANG_FILES=true nuxi dev playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "dev:prepare:multiple-json": "nuxt-module-build build --stub && nuxt-module-build prepare && MULTIPLE_LANG_FILES=true nuxi prepare playground",
    "dev:build": "nuxi build playground",
    "dev:generate": "nuxi generate playground",
    "dev:build:multiple-json": "MULTIPLE_LANG_FILES=true nuxi build playground",
    "dev:generate:multiple-json": "MULTIPLE_LANG_FILES=true nuxi generate playground",
    "dev:preview": "nuxi preview playground",
    "dev:prepare:date-io": "nuxt-module-build build --stub && nuxt-module-build prepare && pnpm nuxt prepare date-io-playground",
    "dev:date-io": "nuxi dev date-io-playground",
    "docs:dev": "pnpm -C docs run dev",
    "docs:build": "nuxt-module-build prepare && pnpm -C docs run build",
    "docs:serve": "pnpm -C docs run serve",
    "lint": "eslint .",
    "lint:fix": "nr lint --fix",
    "publint": "publint",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "release": "bumpp && npm publish"
  },
  "dependencies": {
    "@nuxt/kit": "catalog:",
    "defu": "catalog:",
    "destr": "catalog:",
    "local-pkg": "catalog:",
    "pathe": "catalog:",
    "perfect-debounce": "catalog:",
    "ufo": "catalog:",
    "unconfig": "catalog:",
    "vite-plugin-vuetify": "catalog:",
    "vuetify": "catalog:"
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:",
    "@antfu/ni": "catalog:",
    "@date-io/luxon": "catalog:",
    "@fortawesome/fontawesome-svg-core": "catalog:",
    "@fortawesome/free-solid-svg-icons": "catalog:",
    "@fortawesome/vue-fontawesome": "catalog:",
    "@iconify-json/carbon": "catalog:",
    "@iconify-json/mdi": "catalog:",
    "@mdi/js": "catalog:",
    "@nuxt/devtools": "catalog:",
    "@nuxt/module-builder": "catalog:",
    "@nuxt/schema": "catalog:",
    "@nuxt/test-utils": "catalog:",
    "@nuxtjs/i18n": "catalog:",
    "@parcel/watcher": "catalog:",
    "@types/node": "catalog:",
    "@unocss/nuxt": "catalog:",
    "bumpp": "catalog:",
    "eslint": "catalog:",
    "luxon": "catalog:",
    "nuxt": "catalog:",
    "publint": "catalog:",
    "rimraf": "catalog:",
    "sass": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vitest": "catalog:",
    "vue-tsc": "catalog:"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search"
      ]
    }
  },
  "resolutions": {
    "@nuxt/kit": "catalog:resolutions",
    "vite": "catalog:resolutions",
    "vue": "catalog:resolutions"
  },
  "build": {
    "externals": [
      "@vuetify/loader-shared",
      "node:child_process",
      "node:fs",
      "consola",
      "destr",
      "esbuild",
      "local-pkg",
      "pathe",
      "perfect-debounce",
      "rollup",
      "upath",
      "ufo",
      "unconfig",
      "vite",
      "vite-plugin-vuetify",
      "vuetify"
    ]
  },
  "stackblitz": {
    "installDependencies": false,
    "startCommand": "node .stackblitz.js && pnpm install && nr prepack && nr dev:prepare && nr dev"
  }
}
